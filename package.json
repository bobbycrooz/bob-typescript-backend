{
  "name": "server-template",
  "version": "1.0.0",
  "description": "'backend server templates for bootsrapping backend projects\"",
  "main": "index.js",
  "scripts": {
    "test": "npm run test",
    "build": "npm run clean && tsc",
    "clean": "rimraf ./dist ./logs",
    "start:clean": " npm run build && node ./dist/index.js",
    "start": " node dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon index.ts",
    "dev:neat": "cross-env NODE_ENV=development ts-node-dev --respawn --files --exit-child src/index.ts",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "build:dev": "rimraf ./dist && cross-env NODE_ENV=production tsc"
  },
  "keywords": [
    "['backend'",
    "'ts']"
  ],
  "author": "bobbydev",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.3.1",
    "@types/otp-generator": "^4.0.0",
    "@types/uuid": "^9.0.2",
    "bcrypt": "^5.0.1",
    "nodemon": "^1.14.9",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "africastalking": "^0.6.2",
    "and": "^0.0.3",
    "body-parser": "^1.20.2",
    "cloudinary": "^1.37.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "glob": "^10.3.0",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.3.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "npm": "^9.7.2",
    "otp-generator": "^4.0.1",
    "redis": "^4.6.7",
    "remove": "^0.1.5",
    "twilio": "^4.12.0",
    "uuid": "^9.0.0",
    "winston": "^3.9.0",
    "xss": "^1.0.14"
  }
}
